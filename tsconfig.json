{ 
    "compilerOptions": {
      "target": "es5",
      "module": "commonjs",
      "moduleResolution": "node",
      "sourceMap": true,
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "removeComments": false,
      "noImplicitAny": false,
      "resolveJsonModule": true,
      "outDir": "dist",
      "rootDir": "."
    },
    "exclude": [ "node_modules" ]
  }